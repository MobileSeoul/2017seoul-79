<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/more_line"
    android:orientation="vertical"
    tools:context="com.app.seoul.View.Activity.UserprivitActivity">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="52.66dp"
        android:background="@color/textColor"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/up_back"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="4.67dp"
            android:layout_marginTop="15.33dp"
            android:background="@drawable/settings_back_btn_24x24" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="119dp"
            android:layout_marginTop="17dp"
            android:gravity="center"
            android:text="개인정보 취급방침"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="11.33dp"
        android:background="@drawable/white_round"
        android:text="\n 개인정보 취급방침 내용"
        android:textColor="@color/black"
        android:textSize="13.67sp"
        android:textStyle="bold" />

</LinearLayout>
